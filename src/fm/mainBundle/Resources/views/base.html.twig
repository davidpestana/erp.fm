<!DOCTYPE html>
<!--[if lt IE 7 ]><html class="ie ie6" lang="en"> <![endif]-->
<!--[if IE 7 ]><html class="ie ie7" lang="en"> <![endif]-->
<!--[if IE 8 ]><html class="ie ie8" lang="en"> <![endif]-->
<!--[if (gte IE 9)|!(IE)]><!-->
<html {{ sonata_seo_html_attributes() }}>
<!--<![endif]-->
<head {{ sonata_seo_head_attributes() }}>

<!-- Basic Page Needs
  ================================================== -->
{{ sonata_seo_title() }}
{{ sonata_seo_metadatas() }}
{{ sonata_seo_link_canonical() }}
{{ sonata_seo_lang_alternates() }}
<!-- Mobile Specific Metas
  ================================================== -->
<meta name="viewport" content="width=device-width, initial-scale=1.0">
{% block stylesheets %}{% endblock %}
<link rel="icon" type="image/x-icon" href="/favicon.ico" />
<meta name="google-site-verification" content="CNornoFXlwiR_MMOy_8EIO-5mp2RqBis0jmrZRuWkz8" />

<!--Start Cookie Script-->

<script type="text/javascript" charset="UTF-8" src="http://chs02.cookie-script.com/s/66afb5a543884f4cca173c2e30d578f7.js"></script>

<!--End Cookie Script-->
{% block head %}{% endblock %}

</head>
<body>

    <!-- Primary Page Layout 
      ================================================== -->
    <section class="contactInfos">
      <div class="container">
        <div class="row">
          <div class="span12">
            <p class="pull-right clients">
            <span class="contactMail" style="text-aling:center; margin-right: 5px;"><i class="icon-flag"></i> <a href="#">{% trans %}Empresa{% endtrans %}</a> | <a href="#">{% trans %}Fabricación{% endtrans %}</a> | <a href="{{path('fm_main_paginas_distribuidores')}}">{% trans %}Distribuidores{% endtrans %}</a> |  <a href="{{path('fm_main_paginas_concesionarios')}}">{% trans %}Concesionarios{% endtrans %}</a> | <span style="white-space: nowrap;"><img src="/whatsapp.png">+34 628 137 304</span></span>
            </p>
          </div>
		  
          <div id="flags" class="span12">
            <p class="pull-left">
				{% set locale = app.request.locale %}

				{% if locale == 'es' %}
				<img src="/img/banderas/spain.png" alt="Español">
				{% else %}
				<a href="{{ path('homepage', { '_locale': 'es' }) }}"><img src="/img/banderas/spain.png" alt="Español"></a>
				{% endif %}

				{% if locale == 'en' %}
				<img src="/img/banderas/uk.png" alt="English">
				{% else %}
				<a href="{{ path('homepage', { '_locale': 'en' }) }}"><img src="/img/banderas/uk.png" alt="English"></a>
				{% endif %}

				{% if locale == 'fr' %}
				<img src="/img/banderas/france.png" alt="France">
				{% else %}
				<a href="{{ path('homepage', { '_locale': 'en' }) }}"><img src="/img/banderas/france.png" alt="France"></a>
				{% endif %}

				{% if locale == 'pt' %}
				<img src="/img/banderas/portugal.png" alt="Portugal">
				{% else %}
				<a href="{{ path('homepage', { '_locale': 'en' }) }}"><img src="/img/banderas/portugal.png" alt="Portugal"></a>
				{% endif %}
            </p>

			<p class="pull-right">
			<div class="addthis_sharing_toolbox" style="width:96px; float:right; padding-top:5px"></div>
			</p>

            {#<span class="contactMail"><i class="icon-mail"></i> <a href="#"> contacto@furgomania.com </a></span>
            
            {#<span class="contactPhone"> <i class="icon-phone"></i> 902.120.205{# <img src="/whatsapp.png" /> 628.137.304</span>}#}

          </div>

        </div>
      </div>
    </section>
    <div id="globalWrapper" class="home container">
        {% include 'mainBundle::header.html.twig' only %}

        {% block body %}{% endblock %}

        
    </div>
    {% include 'mainBundle::footer.html.twig' only %}
    {% block javascripts %}{% endblock %}


<script>
  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

  ga('create', 'UA-2347357-5', 'furgomania.com');
  ga('send', 'pageview');

</script>


</body>
</html>