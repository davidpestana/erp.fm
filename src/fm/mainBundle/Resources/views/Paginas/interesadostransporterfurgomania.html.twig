{% extends 'mainBundle::layout.html.twig' %}
{% block title %}Volkswagen Transporter Furgomania{% endblock %}
{% block description %}{% endblock %}
{% block article %}

{% set names = [
'FRANCISCO JAVIER GONZÁLEZ MONTESDEOCA'
]	
%}


<section>
	<div class="row">
        <div class="span12">
			<h1>LISTADO DE INTERESADOS EN LA COMPRA CONJUNTA DE LA VW T6</h1>
			<p>(Actualización del listado semanal)</p>
			<h2>Visita nuestro <a href="https://www.facebook.com/Furgomania/">Facebook</a> y ¡únete!</h2>
		</div>
		
        <div class="span6">
			<ol>
				{% for name in names %}
					<li class="BigText">{{name | title}}</li>
				{% endfor %}
			</ol>
        </div>

        <div class="span6">
          <img class="img-responsive" alt="Volkswagen Transporter Furgomania" src="/img/volkswagen-transporter-furgomania.jpg">
        </div>
		
	</div>
</section>


{% endblock %}